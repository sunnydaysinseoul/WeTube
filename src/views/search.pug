//- URL : /search
extends base.pug
include mixins/video.pug 
block content
    //- include partials/search.pug
    i.fa-solid.fa-magnifying-glass.pageTitle
    span.pageTitle #{pageTitle} for : #{search}
    .card-list-container
        .card-list
            each video in videos
                +videoMxn(video)
            else
                li Sorry, nothing found.