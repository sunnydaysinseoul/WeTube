//- URL : /videos/:vId 
extends base.pug
block headerContent
    include partials/header.pug
block content
    small #{video.views}#{video.views === 1 ? "view" : "views"} | Created: #{video.createdDate} | Rating : #{video.ratings}/5
    h5 #{video.hashtags}
    p=video.description
    a(href=`${video.id}/edit`)
        button Edit Video

